{
  "name": "wifi-firewall",
  "version": "1.0.0",
  "description": "High-speed WiFi firewall for controlling website access",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/api-server.js",
    "dev": "nodemon src/api-server.js",
    "frontend": "npm run dev --prefix frontend",
    "dev:all": "concurrently \"npm run dev\" \"npm run frontend\"",
    "install-service": "sudo node scripts/install-service.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dns-packet": "^5.6.1",
    "dns2": "^2.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "ip": "^1.1.8",
    "redis": "^4.6.13",
    "socket.io": "^4.6.2"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.0.3"
  }
}
